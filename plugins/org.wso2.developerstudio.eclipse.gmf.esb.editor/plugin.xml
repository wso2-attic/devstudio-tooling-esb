<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated esb -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="org.wso2.developerstudio.eclipse.gmf.esb.presentation.EsbModelWizardID"
            name="%_UI_EsbModelWizard_label"
            class="org.wso2.developerstudio.eclipse.gmf.esb.presentation.EsbModelWizard"
