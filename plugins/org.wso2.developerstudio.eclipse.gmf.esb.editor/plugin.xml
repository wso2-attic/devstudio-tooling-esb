         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated esb -->
      <editor
            id="org.wso2.developerstudio.eclipse.gmf.esb.presentation.EsbEditorID"
            name="%_UI_EsbEditor_label"
            icon="icons/full/obj16/EsbModelFile.gif"
            extensions="esb"
            class="org.wso2.developerstudio.eclipse.gmf.esb.presentation.EsbEditor"
            contributorClass="org.wso2.developerstudio.eclipse.gmf.esb.presentation.EsbActionBarContributor">
      </editor>
   </extension>

</plugin>
